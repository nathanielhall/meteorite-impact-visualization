{
  "name": "meteorite-impact-visualization",
  "version": "1.0.0",
  "description": "React application for visualizing Earth meteorite impact locations",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --open --mode=development",
    "test": "jest",
    "tests:watch": "jest --watch",
    "prettier": "prettier --write \"./**/*.{json,js,jsx,scss}\""
  },
  "keywords": [
    "react",
    "meteorite",
    "impact",
    "visualization"
  ],
  "author": "Nathaniel C. Hall",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "date-fns": "^2.10.0",
    "leaflet": "^1.6.0",
    "react-leaflet": "^2.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^components(.*)$": "<rootDir>/src/components$1"
    },
    "modulePaths": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "./enzyme.config.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.jsx?$"
  }
}
